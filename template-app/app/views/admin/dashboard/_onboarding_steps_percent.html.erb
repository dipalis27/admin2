<div class="onboarding_heading">
    <!-- stroke-dasharray -- 0% : 0,251.2 || 50% : 125.6,125.6 || 100% : 251.2,0 -->
    <svg class="onboarding_svg" viewbox="0 0 100 100">
      <circle cx="50" cy="50" r="45" fill="#fff"/>
      <% x = 2.512 * percent %>
      <% y = 251.2 - x %>
      <path fill="none" stroke-linecap="round" stroke-width="8" stroke="#00D659"
      <%= "stroke-dasharray= #{x},#{y}" %> d="M50 10 a 40 40 0 0 1 0 80 a 40 40 0 0 1 0 -80"/>
      <text x="50" y="50" text-anchor="middle" dy="7" font-size="20"><%= percent.floor %>%</text>
    </svg>
  </div>

